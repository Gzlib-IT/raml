{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "description": "Collection of instances",
  "properties": {
    "instances": {
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "instance.json"
      }
    },
    "totalRecords": {
      "type": "integer"
    },
    "resultInfo" : {
      "$ref" : "resultInfo.schema"
    }
  },
  "required": [
    "instances",
    "totalRecords"
  ]
}
